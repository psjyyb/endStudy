<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yedam.app.feed.mapper.FeedMapper">
	<select id="selectFeedList" resultType="FeedVO">
		SELECT member_no
		      ,feed_no
		      ,feed_content
		      ,feed_hashtag
		      ,feed_write_date
		      ,feed_revision_date
		FROM feed
	</select>
</mapper>